{
    "name": "lcobucci/chimera-routing",
    "description": "A collection of reusable PSR-15 components that connects Chimera to any framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "lcobucci/chimera-foundation": "^1.0@alpha",
        "lcobucci/content-negotiation-middleware": "^1.0",
        "middlewares/negotiation": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^3.0",
        "infection/infection": "^0.7",
        "phpstan/phpstan": "^0.10@dev",
        "phpstan/phpstan-phpunit": "^0.10@dev",
        "phpstan/phpstan-strict-rules": "^0.10@dev",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.2",
        "zendframework/zend-diactoros": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\Chimera\\Routing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\Chimera\\Routing\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
